{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "full": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "",
      "sourceRoot": "full/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/full",
            "index": "full/src/index.html",
            "main": "full/src/main.ts",
            "polyfills": "full/src/polyfills.ts",
            "tsConfig": "full/tsconfig.prod.json",
            "assets": [
              "full/src/favicon.ico", 
              "full/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/hslayers-ng/src/assets",
                "output": "./assets/hslayers-ng/"
              }
            ],
            "preserveSymlinks": true,
            "styles": [
              "./node_modules/hslayers-ng/css/hslayers-ng.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "development": {
              "tsConfig": "full/tsconfig.dev.json"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "namedChunks": false,
              "statsJson": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "full:build:development"
          },
          "configurations": {
            "production": {
              "browserTarget": "full:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "full:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "full/test.ts",
            "polyfills": "full/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "full/favicon.ico"
            ],
            "styles": [
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "full:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "full:serve:production"
            }
          }
        }
      }
    },
    "routing": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "",
      "sourceRoot": "routing/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/routing",
            "index": "routing/src/index.html",
            "main": "routing/src/main.ts",
            "polyfills": "routing/src/polyfills.ts",
            "tsConfig": "routing/tsconfig.prod.json",
            "assets": [
              "routing/src/favicon.ico", 
              "routing/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/hslayers-ng/src/assets",
                "output": "./assets/hslayers-ng/"
              }    
            ],
            "preserveSymlinks": true,
            "styles": [
              "./node_modules/hslayers-ng/css/hslayers-ng.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "development": {
              "tsConfig": "routing/tsconfig.dev.json"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "statsJson": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "routing:build:development"
          },
          "configurations": {
            "production": {
              "browserTarget": "routing:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "routing:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "routing/test.ts",
            "polyfills": "routing/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "routing/src/favicon.ico"
            ],
            "styles": [
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "routing:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "routing:serve:production"
            }
          }
        }
      }
    },
    "cesium": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "",
      "sourceRoot": "cesium/src/",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "preserveSymlinks": true,
            "outputPath": "dist/cesium",
            "index": "cesium/src/index.html",
            "main": "cesium/src/main.ts",
            "polyfills": "cesium/src/polyfills.ts",
            "tsConfig": "cesium/tsconfig.json",
            "aot": true,
            "optimization": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "cesium/src/assets",
                "output": "./assets"
              },
              "cesium/src/favicon.ico", 
              "cesium/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/hslayers-ng/src/assets",
                "output": "./assets/hslayers-ng/"
              },    
              {
                "glob": "**/*",
                "input": "./node_modules/cesium/Source/Assets",
                "output": "./assets/cesium/Assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/cesium/Source/Widgets",
                "output": "./assets/cesium/Widgets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/cesium/Source/Workers",
                "output": "./assets/cesium/Workers"
              }
            ],
            "styles": [
              "node_modules/ol/ol.css",
              "node_modules/cesium/Build/Cesium/Widgets/widgets.css",
              "./node_modules/hslayers-ng/css/hslayers-ng.css"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "cesium/custom-webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6.5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "cesium:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cesium:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cesium:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "cesium/test/test.ts",
            "polyfills": "cesium/src/polyfills.ts",
            "tsConfig": "cesium/tsconfig.spec.json",
            "karmaConfig": "cesium/karma.conf.js",
            "assets": [
              "cesium/src/favicon.ico",
              "cesium/src/assets"
            ],
            "styles": [
              "cesium/src/styles.sass"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cesium:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cesium:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "full",
  "cli": {
    "analytics": false
  }
}